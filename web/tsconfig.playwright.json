{
    "extends": "./tsconfig.app.json",
    "compilerOptions": {
        "composite": true,
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.playwright.tsbuildinfo",
        "types": [
            "node",
            "@playwright/test",
            "vite/client"
        ]
    },
    "include": [
        "playwright.config.ts",
        "tests/**/*.ts",
        "src/**/*.ts",
        "src/**/*.tsx",
        "src/**/*.vue",
        "src/**/*.d.ts"
    ]
}